<%= form_for @client do |f| %>

<% if @client.errors.any? %>
<div id="error_explanation">
    <h2><%= pluralize(@client.errors.count, "error") %> prohibited
        this client from being saved:</h2>
    <ul>
        <% @client.errors.full_messages.each do |msg| %>
